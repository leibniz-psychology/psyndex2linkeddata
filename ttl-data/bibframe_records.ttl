works:0396669 a bf:Work ;
    bf:hasInstance instances:0396669 ;
    bf:identifiedBy [ a bf:Local,
                pxc:DFK ;
            bf:source [ a bf:Source ;
                    bf:code "ZPID.PSYNDEX.DFK" ] ;
            rdf:value "0396669" ] ;
    bf:language lang:ger ;
    bf:summary [ a pxc:Abstract ;
            rdfs:label "Was kann die Kinder- und Jugendhilfe zum Kinder- und Jugendschutz beitragen? Und was ist nötig, um den hohen Anforderungen gerecht zu werden? Unsere Gesellschaft und das Rechtssystem gehen von der Vorannahme aus, dass alle Eltern ihre Kinder ausreichend pflegen, fördern und unterstützen können. Eltern brauchen ihre Erziehungsfähigkeit nicht zu beweisen. Sie müssen keine Kurse belegen und sich nicht vorab qualifizieren. Trotzdem ist das Wohl der Kinder auch eine gesellschaftliche Aufgabe und Verantwortung. Es gibt eine Vielzahl von Hilfsangeboten, die Eltern, Kinder und Jugendliche erreichen sollen, um dieser Verantwortung gerecht zu werden."@de ;
            bf:adminMetadata [ a bf:AdminMetadata ;
                    bflc:metadataLicensor "Original" ] ] .

works:0396670 a bf:Work ;
    bf:hasInstance instances:0396670 ;
